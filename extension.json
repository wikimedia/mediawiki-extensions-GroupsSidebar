{
	"name": "GroupsSidebar",
	"version": "0.3.0",
	"author": "Church of emacs",
	"url": "https://www.mediawiki.org/wiki/Extension:GroupsSidebar",
	"descriptionmsg": "groupssidebar-desc",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.39.4"
	},
	"MessagesDirs": {
		"GroupsSidebar": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"GroupsSidebar": "GroupsSidebar.php"
	},
	"HookHandlers": {
		"GroupsSidebarHooks": {
			"class": "GroupsSidebar",
			"services": [
				"UserGroupManager"
			]
		}
	},
	"Hooks": {
		"SidebarBeforeOutput": "GroupsSidebarHooks"
	},
	"manifest_version": 2
}
